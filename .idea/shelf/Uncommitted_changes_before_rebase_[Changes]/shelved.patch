@@ -1,397 +0,0 @@
-/*THIS IS THE SASS STYLE SHEET OF THE COMPTE PAGE*/
-/*---------IMPORTS----------*/
-/*COMMON SASS FILE FOR RULES APPLYING TO ALL*/
-@font-face {
-  font-family: Maagkramp;
-  src: url("./fonts/Maagkramp/maagkramp.ttf");
-}
-html, body {
-  margin: 0;
-  padding: 0;
-  width: 100%;
-}
-
-body {
-  display: flex;
-  flex-direction: column;
-  align-items: center;
-  color: #112b3c;
-  background-color: #efefef;
-  font-family: Maagkramp, sans-serif;
-}
-
-a {
-  color: #112b3c;
-  text-decoration: none;
-}
-
-/*-----------HEADER-----------*/
-header {
-  padding: 0.5rem 1rem 0 1rem;
-  display: flex;
-  height: 5rem;
-  width: calc(100% - 2rem);
-}
-header nav {
-  width: 100%;
-  height: 100%;
-  display: flex;
-  justify-content: end;
-}
-header nav > div {
-  height: 100%;
-  display: inline-flex;
-  justify-content: center;
-  align-items: center;
-  gap: 1.5rem;
-  padding-right: 3rem;
-}
-header h1, header a {
-  margin: 0;
-  padding: 0;
-  text-decoration: none;
-  color: #112b3c;
-  font-size: 1.5rem;
-}
-
-#burger-container {
-  position: relative;
-}
-#burger-container svg {
-  height: 3rem;
-}
-
-#burger-nav {
-  display: none;
-  position: absolute;
-  left: -6.5rem;
-  top: 5rem;
-  min-width: fit-content;
-  width: 11.5rem;
-  height: 50vh;
-  background-color: #CCCCCC;
-  border-radius: 5px;
-}
-
-.burger-active + #burger-nav {
-  display: inline-flex;
-  flex-direction: column;
-  justify-content: center;
-  align-items: baseline;
-  gap: 2rem;
-  z-index: 10;
-  padding-left: 1rem;
-  opacity: 1;
-  transition: easeAnimation 1s ease-out;
-}
-.burger-active + #burger-nav a {
-  min-width: fit-content;
-}
-
-/*--------------MAIN-------------*/
-main {
-  min-height: 100vh;
-}
-
-.hidden {
-  visibility: hidden !important;
-}
-
-.make-disappear {
-  display: none !important;
-}
-
-/*-----------FORM--------------*/
-form {
-  display: flex;
-  flex-direction: column;
-  width: 30%;
-  gap: 0.3rem;
-}
-
-input[type=submit] {
-  width: 35%;
-  border: 2px solid transparent;
-  background: #f66b0e;
-}
-input[type=submit]:hover {
-  background-color: #efefef;
-  border: 2px solid #e0383e;
-}
-
-input, textarea {
-  padding: 0.5rem;
-  border-radius: 0.5rem;
-}
-input:not(input[type=submit]), textarea:not(input[type=submit]) {
-  border: 1px solid #f66b0e;
-  width: 100%;
-}
-
-input, textarea {
-  background-color: #efefef;
-  font-size: large;
-  color: rgba(17, 43, 60, 0.7);
-}
-input:hover::placeholder, textarea:hover::placeholder {
-  color: #f66b0e;
-}
-input::placeholder, textarea::placeholder {
-  font-family: Maagkramp, sans-serif;
-}
-
-select, option {
-  background-color: #efefef;
-  color: rgba(17, 43, 60, 0.44);
-  height: 2.2rem;
-  border: 1px solid #f66b0e;
-  padding: 0.5rem;
-  border-radius: 0.5rem;
-  font-family: Maagkramp, sans-serif;
-  font-size: 1.2rem;
-}
-select:hover, option:hover {
-  color: #f66b0e;
-}
-
-.all-generated-cards {
-  min-height: fit-content;
-  width: 80%;
-  padding: 5vh 10% 0 10%;
-  display: flex;
-  justify-content: center;
-  flex-basis: 30%;
-  flex-wrap: wrap;
-  gap: 2rem;
-}
-
-.generated-card {
-  display: flex;
-  flex-direction: column;
-  justify-content: center;
-  align-items: center;
-  gap: 0.3rem;
-  height: 12rem;
-  min-width: fit-content;
-  width: 15rem;
-  padding: 0.5rem;
-  background-color: #f66b0e;
-  border-radius: 10px;
-}
-.generated-card:hover {
-  opacity: 0.7;
-  cursor: pointer;
-}
-
-/*-----------FOOTER------------*/
-footer {
-  width: 100%;
-  height: fit-content;
-  margin-top: 5rem;
-  background-color: #efefef;
-  display: flex;
-  align-items: end;
-  color: #112b3c;
-  font-size: 1.8em;
-  flex-grow: 1;
-}
-footer span {
-  text-decoration: none;
-  color: #112b3c;
-}
-footer a:hover {
-  opacity: 0.7;
-}
-footer a:active {
-  color: #f66b0e;
-}
-
-#footer_infos_container {
-  width: 100%;
-  background-color: #EBEBEB;
-  padding-top: 1rem;
-}
-
-#footer_infos_titles {
-  width: 100%;
-  display: flex;
-  justify-content: space-around;
-  margin-bottom: 2rem;
-}
-
-#footer_infos_descriptions {
-  width: 100%;
-  display: flex;
-  justify-content: space-around;
-}
-#footer_infos_descriptions div {
-  width: 20%;
-  min-width: fit-content;
-  display: flex;
-  flex-direction: column;
-  gap: 1rem;
-}
-
-/*--------ANIMATIONS---------*/
-/*--------Some general animation---------*/
-a.underlined {
-  position: relative;
-  transition: 0.3s;
-}
-a.underlined::after {
-  content: "";
-  position: absolute;
-  height: 0.35rem;
-  width: 60%;
-  bottom: -0.5rem;
-  left: 0;
-  border-radius: 10px;
-  transition: 0.25s;
-  background-color: transparent;
-}
-a.underlined:hover::after {
-  background-color: #f66b0e;
-}
-a.underlined.nav-txt-active::after {
-  background-color: #f66b0e;
-}
-
-#animated-burger {
-  fill: #112b3c;
-}
-#animated-burger:hover {
-  fill: #f66b0e;
-}
-
-.burger-active {
-  overflow: visible;
-  transition: 0.3s;
-  transform-origin: center;
-  transform: rotate(90deg);
-}
-.burger-active path:nth-child(1) {
-  transform: translateY(2rem);
-}
-.burger-active path:nth-child(3) {
-  transform: translateY(-2rem);
-}
-
-@keyframes pulseMap {
-  0% {
-    opacity: 0;
-    transform: scale(0);
-  }
-  25% {
-    opacity: 0.4;
-  }
-  100% {
-    transform: scale(1);
-  }
-}
-@keyframes pulse {
-  25% {
-    opacity: 0.4;
-  }
-  100% {
-    transform: scale(1);
-  }
-}
-@keyframes easeAnimation {
-  0% {
-    opacity: 0;
-    display: none;
-  }
-  50% {
-    opacity: 0.5;
-    display: inline-flex;
-  }
-  100% {
-    opacity: 1;
-    display: inline-flex;
-  }
-}
-@keyframes floating {
-  0% {
-    transform: translatey(0);
-  }
-  50% {
-    transform: translatey(-10px);
-  }
-  100% {
-    transform: translatey(0);
-  }
-}
-/*---------RESPONSSIVE----------*/
-@media (max-width: 767px) {
-  header {
-    height: 2.5rem;
-  }
-  header nav > div {
-    padding-right: 0;
-  }
-  header nav > div > a {
-    display: none;
-  }
-  header nav > div > a#deconnect-nav {
-    display: inline-block;
-  }
-  header nav > div #burger-nav {
-    overflow: visible;
-    left: calc(-90vw + 4rem);
-    width: calc(80vw - 1rem);
-  }
-  header nav > div .burger-active + #burger-nav {
-    align-items: center;
-  }
-  footer {
-    font-size: 1.2rem;
-  }
-  #all-generated-cards {
-    flex-basis: 100%;
-  }
-}
-@media (max-width: 1023px) {
-  form {
-    width: 85% !important;
-  }
-}
-@media (min-width: 768px) and (max-width: 1023px) {
-  a#deconnect-nav {
-    display: none;
-  }
-  #all-generated-cards {
-    flex-basis: 50%;
-  }
-}
-/*---------GENERAL STUFF----*/
-/*--------MAIN----------*/
-main {
-  height: fit-content;
-  padding: 2rem 0 2rem 0;
-}
-
-h1 {
-  background-color: rgba(32, 83, 117, 0.35);
-  width: 90vw;
-  margin-left: 10vw;
-}
-
-h2, p {
-  font-family: "Source Code Pro", "SF Mono", Monaco, Inconsolata, "Fira Mono", "Droid Sans Mono", monospace, monospace;
-}
-
-h2 {
-  text-align: center;
-}
-
-#recipes-single-card-container {
-  width: 100%;
-  height: fit-content;
-  display: flex;
-  justify-content: center;
-}
-#recipes-single-card-container p {
-  flex-basis: 70%;
-}
-
-/*# sourceMappingURL=style-single-recipes.css.map */
